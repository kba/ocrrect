{
  "name": "ocrrect",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "find src -name '*.coffee' -exec coffee {} \\; | tap-spec"
  },
  "author": "",
  "license": "ISC",
  "coffeelintConfig": {
    "max_line_length": {
      "value": 120,
      "level": "error",
      "limitComments": true
