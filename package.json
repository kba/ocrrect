    },
    "indentation": {
      "value": 1,
      "level": "error"
    }
  },
  "crojsdoc": {
    "theme": "kba",
    "theme_opts": {
      "bootswatch": "spacelab"
    }
  },
  "devDependencies": {
    "crojsdoc": "git+https://github.com/kba/crojsdoc.git",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "app-root-path": "^1.3.0",
    "async": "^1.5.2",
    "cheerio": "^0.22.0",
    "csv-parse": "^1.1.1",
    "mkdirp": "^0.5.1",
    "superagent": "^2.0.0",
    "traf": "0.0.10",
    "xmldom": "^0.1.22",
    "xmljs": "^0.3.2",
    "xpath": "0.0.23",
    "xregexp": "^3.1.1"
  }
